{
  "name": "docx-node-service",
  "version": "1.0.0",
  "main": "index.js",
  "author": "Jean Barriere <jean@barriere.io>",
  "scripts": {
    "start": "node index.js",
    "netlify": "netlify-lambda serve .netlify/functions",
    "postinstall": "netlify-lambda install",
    "netlify:build": "netlify-lambda build .netlify/functions"
