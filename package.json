    "cors": "^2.8.5",
    "docxtemplater": "^3.19.7",
    "express": "^4.17.1",
    "firebase-admin": "^9.4.1",
    "firebase-functions": "^3.11.0",
    "helmet": "^4.2.0",
    "lambda-multipart-parser": "^0.0.2",
    "multer": "^1.4.2",
    "netlify-lambda": "^2.0.1",
    "pizzip": "^3.0.6"
  }
}
